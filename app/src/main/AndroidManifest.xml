<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.janit.capstoneproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".creditCard"
            android:label="@string/title_activity_credit_card"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".studentloan"
            android:label="@string/title_activity_studentloan"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".car"
            android:label="@string/title_activity_car"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".house"
            android:label="@string/title_activity_house"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>